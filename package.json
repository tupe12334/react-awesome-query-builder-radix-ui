{
  "name": "react-awesome-query-builder-radix-ui",
  "version": "0.1.0",
  "description": "User-friendly query builder for React. Radix UI widgets",
  "keywords": [
    "query-builder",
    "query",
    "builder",
    "react",
    "reactjs",
    "ui",
    "radix",
    "radix-ui"
  ],
  "homepage": "https://github.com/tupe12334/react-awesome-query-builder-radix-ui",
  "bugs": "https://github.com/tupe12334/react-awesome-query-builder-radix-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/tupe12334/react-awesome-query-builder-radix-ui.git"
  },
  "license": "MIT",
  "author": "Ofek Gabay <ofek.gabay.he@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs",
    "esm",
    "types",
    "css",
    "modules"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./package.json": "./package.json",
    "./css/styles.css": "./css/styles.css",
    "./css/styles.scss": "./css/styles.scss"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "node ./scripts/build-npm.js",
    "eslint": "eslint modules/",
    "lint": "pnpm eslint && pnpm tsc",
    "lint:fix": "eslint modules/ --fix",
    "format": "prettier --write \"modules/**/*.{ts,tsx,js,jsx,json,md}\" \"*.{json,md}\"",
    "format:check": "prettier --check \"modules/**/*.{ts,tsx,js,jsx,json,md}\" \"*.{json,md}\"",
    "spell": "cspell lint '**/*.{ts,tsx,js,jsx,md,json}' --gitignore",
    "spell:check": "cspell lint '**/*.{ts,tsx,js,jsx,md,json}' --gitignore",
    "prepare": "husky",
    "release": "release-it",
    "tsc": "tsc -p . --noEmit",
    "tsc-emit-types": "tsc -p . --declaration --emitDeclarationOnly --allowJs false --declarationDir types"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "@react-awesome-query-builder/ui": "^6.6.15",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react-number-format": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "cspell": "^9.3.1",
    "eslint": "^9.39.1",
    "eslint-config-agent": "^1.8.4",
    "eslint-plugin-package-json": "^0.74.0",
    "globby": "^14.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "release-it": "^19.0.6",
    "sass": "^1.77.2",
    "typescript": "~5.4.5"
  },
  "peerDependencies": {
    "@radix-ui/react-checkbox": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.0.0",
    "@radix-ui/react-switch": "^1.0.0",
    "@radix-ui/react-toggle": "^1.0.0",
    "@radix-ui/react-toggle-group": "^1.0.0",
    "react": "^16.8.4 || ^17.0.1 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.4 || ^17.0.1 || ^18.0.0 || ^19.0.0"
  }
}
